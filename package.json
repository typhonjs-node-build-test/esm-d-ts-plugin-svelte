{
  "name": "@typhonjs-build-test/esm-d-ts-plugin-svelte",
  "description": "`esm-d-ts` plugin to generate declarations for ES Module based Svelte 4 components",
  "version": "0.0.0",
  "author": "Michael Leahy <support@typhonjs.io> (https://github.com/typhonrt)",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.js"
    },
    "./package.json": "./package.json"
  },
  "contributors": [
    "Michael Leahy <support@typhonjs.io> (https://github.com/typhonrt)"
  ],
  "dependencies": {
  },
  "devDependencies": {
    "@typhonjs-typedoc/typedoc-pkg": "^0.0.3",
    "@typhonjs-config/eslint-config" : "^0.6.0",
    "eslint": "^8.56.0"
 },
  "peerDependencies": {
    "@typhonjs-build-test/esm-d-ts" : "github:typhonjs-node-build-test/esm-d-ts#0.2.3-dev",
    "svelte": "4",
    "svelte2tsx": ">=0.7.0"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "/src"
  ],
  "homepage": "https://github.com/typhonjs-node-build-test/esm-d-ts-plugin-svelte#readme",
  "keywords": [
    "typhonjs",
    "build",
    "esm-d-ts",
    "declarations",
    "plugin",
    "svelte"
  ],
  "license": "MPL-2.0",
  "repository": "github:typhonjs-node-build-test/esm-d-ts-plugin-svelte",
  "bugs": "https://github.com/typhonjs-node-build-test/esm-d-ts-plugin-svelte/issues",
  "apidocs": "https://typhonjs-node-build-test.github.io/esm-d-ts-plugin-svelte/",
  "scripts": {
    "eslint": "eslint .",
    "types": "esm-d-ts gen src/index.js",
